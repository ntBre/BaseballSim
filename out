    $(function() {
        var device = (window.DeviceType || {}).device || 'unknown'
        var attributeSelector = 'data-'+device+'-href'
        $('a[' + attributeSelector + ']').on('touchstart mousedown', function(e) {
            var newHref = e.currentTarget.getAttribute(attributeSelector)
            e.currentTarget.href = newHref
        })
    })
