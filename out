<script charset="utf-8" type="text/javascript">
    var team_id = '';
    var current_season = '2018';
    var DEFAULT_GAME_TYPE = '';
    var QUOTED_DEFAULT_GAME_TYPE="'"+DEFAULT_GAME_TYPE+"'";
    var current_game_type=DEFAULT_GAME_TYPE;

    $(document).ready(function() {

        if(window.DeviceType.device && window.DeviceType.device === 'phone') {
            if(team_id) {
                window.location.href = "http://m.mlb.com/stats?c_id=" + team_id;
            } else {
                window.location.href = "http://m.mlb.com/stats";
            }
        }

        $.getJSON("/gen/content/tag/v1/key-value/stats_config/mlb/general-v1.json").done(function(json) {

            if(team_id) {
                DEFAULT_GAME_TYPE = json["sortable_club_default_game_type"]["t" + team_id];
            } else {
                DEFAULT_GAME_TYPE = json["sortable_sport_default_game_type"]["mlb"];
            }

            QUOTED_DEFAULT_GAME_TYPE="'"+DEFAULT_GAME_TYPE+"'";
            current_game_type=DEFAULT_GAME_TYPE;

            init();

            $(function() {
                $(window).trigger( "hashchange" );
            });

        }).fail(function( jqxhr, textStatus, error ) {

            init();

            $(function() {
                $(window).trigger( "hashchange" );
            });
        });

    });

    function init() {
        var qs = $.deparam.querystring();
        var c;
        if (qs.c_id) {
            c = qs.c_id;
            $(".tabs a.tab_top").each(function(){
                var $tab = $(this);
                var id = $tab.attr('id');
                if (id != 'top_scTab_top_level'){
                    $tab.querystring({"c_id":c});
                }
            });
        }

        $("#stats_prev.stats_prev_default").hover(
                function() {
                    if (!$(this).hasClass("stats_prev_disabled")) {
                        $(this).removeClass("stats_prev_default").addClass("stats_prev_hover");
                    }
                },
                function() {
                    if (!$(this).hasClass("stats_prev_disabled")) {
                        $(this).removeClass("stats_prev_hover").addClass("stats_prev_default");
                    }
                }
        );
        $("#stats_next.stats_next_default").hover(
                function() {
                    if (!$(this).hasClass("stats_next_disabled")) {
                        $(this).removeClass("stats_next_default").addClass("stats_next_hover");
                    }
                },
                function() {
                    if (!$(this).hasClass("stats_next_disabled")) {
                        $(this).removeClass("stats_next_hover").addClass("stats_next_default");
                    }
                }
        );


        $("#stats_next").bind("click", function() {
            if ($(this).hasClass("stats_next_disabled")) {
                return false;
            } else {
                if ($("#toggleMe").length > 0) {
                    $("#toggleMe").remove();
                }
                var css = document.createElement('style');
                css.setAttribute('type', 'text/css');
                css.setAttribute('id','toggleMe');
                var style_string = "";
                var dg = $("#datagrid table");
                var stats_prev = $("#stats_prev");
                var stats_next = $("#stats_next");
                var sectionType = window.sortablePlayerList.cfg.sectionType;
                var statType = window.sortablePlayerList.cfg.statType;
                var season_type = window.sortablePlayerList.cfg.season_type;
                var next_stats_arr;
                if (sectionType === "sp") {
                    next_stats_arr = window.sortablePlayerList.dataConfigs[sectionType][statType][season_type].next_stats_arr;
                } else if (sectionType === "st") {
                    next_stats_arr = window.sortablePlayerList.dataConfigs[sectionType][statType].next_stats_arr;
                }
                var display_idx = parseInt(window.sortablePlayerList.cfg.extended, 10);
                var visible_classes,hidden_classes;
                var v, j, h, next_stats_arr_len;
                var next_stats_arr_len = next_stats_arr.length;
                display_idx = parseInt(display_idx, 10) + 1;
                if (display_idx >= (next_stats_arr.length - 1)) {
                    stats_next.removeClass("stats_next_default").addClass("stats_next_disabled");
                    display_idx = next_stats_arr.length - 1;
                } else {
                    stats_next.removeClass("stats_next_disabled").addClass("stats_next_default");
                }
                if (display_idx <= 0) {
                    stats_prev.removeClass("stats_prev_default").addClass("stats_prev_disabled");
                    display_idx = 0;
                } else {
                    stats_prev.removeClass("stats_prev_disabled").addClass("stats_prev_default");
                }
                for (j = 0; j < next_stats_arr_len; j++) {
                    if (j === display_idx) {
                        visible_classes = next_stats_arr[j];
                        style_string += visible_classes + " {display:table-cell;}\n";
                    } else {
                        hidden_classes = next_stats_arr[j];
                        style_string += hidden_classes + " {display:none;}";
                    }
                }
                window.sortablePlayerList.cfg.extended = display_idx;
                if (css.styleSheet) {
                    css.styleSheet.cssText = style_string;
                } else {
                    css.appendChild(document.createTextNode(style_string));
                }
                $('body').append(css);
            }
        });

        $("#stats_prev").bind("click", function() {
            if ($(this).hasClass("stats_prev_disabled")) {
                return false;
            } else {
                if ($("#toggleMe").length > 0) {
                    $("#toggleMe").remove();
                }
                var css = document.createElement('style');
                css.setAttribute('type', 'text/css');
                css.setAttribute('id','toggleMe');
                var style_string = "";

                var dg = $("#datagrid table");
                var stats_prev = $("#stats_prev");
                var stats_next = $("#stats_next");
                var sectionType = window.sortablePlayerList.cfg.sectionType;
                var statType = window.sortablePlayerList.cfg.statType;
                var season_type = window.sortablePlayerList.cfg.season_type;

                var next_stats_arr;
                if (sectionType === "sp") {
                    next_stats_arr = window.sortablePlayerList.dataConfigs[sectionType][statType][season_type].next_stats_arr;
                } else if (sectionType === "st") {
                    next_stats_arr = window.sortablePlayerList.dataConfigs[sectionType][statType].next_stats_arr;
                }
                var visible_classes,hidden_classes;
                var display_idx = parseInt(sortablePlayerList.cfg.extended, 10);
                var j, v, h;
                display_idx = parseInt(display_idx, 10) - 1;
                if (display_idx <= 0) {
                    stats_prev.removeClass("stats_prev_default").addClass("stats_prev_disabled");
                    display_idx = 0;
                } else {
                    stats_prev.removeClass("stats_prev_disabled").addClass("stats_prev_default");
                }
                if (display_idx >= (next_stats_arr.length - 1)) {
                    stats_next.removeClass("stats_next_default").addClass("stats_next_disabled");
                    display_idx = next_stats_arr.length - 1;
                } else {
                    stats_next.removeClass("stats_next_disabled").addClass("stats_next_default");
                }

                for (j = 0; j < next_stats_arr.length; j++) {
                    if (j === display_idx) {
                        visible_classes = next_stats_arr[j];

                        style_string += visible_classes + " {display:table-cell;}\n";

                    } else {
                        hidden_classes = next_stats_arr[j];

                        style_string += hidden_classes + " {display:none;}";

                    }
                }
                window.sortablePlayerList.cfg.extended = display_idx;
                if (css.styleSheet) {
                    css.styleSheet.cssText = style_string;
                } else {
                    css.appendChild(document.createTextNode(style_string));
                }
                $('body').append(css);
            }
        });
    }

    $("#masthead ul.links_container li[id*='header_stats'] a").live("click",function(evt){

        var elem = $(evt.target),
                href = evt.target.href,
                panelId = href.indexOf('#'),
                paramObj = {},
                parent_id = href.substr(href.indexOf('#')),
                parent_val = parent_id.substring(1);

        if (!panelId || panelId === -1) {
            return true;
        } else {
            evt.preventDefault();
            paramObj = $.deparam.fragment(href);
            parent_id = parent_id.split("=")[1];

            var d_cfg = {
                sectionType:      'sp',
                playerType:       'QUALIFIER',
                statType:         'hitting',
                page_type:        'SortablePlayer',
                season:           window.current_season,
                season_type:      'ANY',
                sportCode:        "'mlb'",
                league_code:       "'MLB'",
                split:			  '',
                team_id:          window.team_id,
                active_sw:        "",
                game_type:        "'"+window.current_game_type+"'",
                position:         "",
                sortOrder:        "'desc'",
                sortColumn:       'avg',
                results:          '',
                page:             1,
                perPage:          50,
                timeframe:        '',
                extended:         0,
                last_x_days:      ''
            };


            window.sortablePlayerList = null;
            $.extend(d_cfg,paramObj);
            updateBbqHash(d_cfg);
            sortablePlayerList = new bam.stats.app.SortablePlayerList(d_cfg);
            sortablePlayerList.load();

        }
    });

    writeAds({sizes : ["1x1"], customparams : {"dcopt":"ist"}});

</script>
</div>
</div>
